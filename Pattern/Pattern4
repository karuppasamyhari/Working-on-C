/*
5 5 5 5 5 
4 4 4 4
3 3 3
2 2
1
*/

#include<stdio.h>
#include<stdlib.h>
int main(){
    int number;
    int row;
    int col;
    number = 5;
    for(row = number; row >= 1; row -= 1){
        for(col = 1; col <= row; col += 1){
        printf("%d ",row);
        }
    printf("\n");
    }
    return EXIT_SUCCESS;
}
